{
  "name": "sentry-zustand-middleware",
  "version": "3.0.4",
  "description": "Zustand middleware to log state and actions on Sentry",
  "keywords": [
    "middleware",
    "sentry",
    "zustand"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Thanaen/sentry-zustand-middleware"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "run-s build:*",
    "build:clean": "rimraf dist",
    "build:types": "tsc -p tsconfig.build.json --emitDeclarationOnly",
    "build:js": "esbuild index.ts --minify --outfile=dist/index.js",
    "prepack": "npm run build",
    "test": "jest",
    "prepare": "husky install",
    "lint": "run-p lint:*",
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier --check .",
    "lint-fix": "run-p lint-fix:*",
    "lint-fix:eslint": "eslint --fix .",
    "lint-fix:prettier": "prettier --write .",
    "release": "standard-version"
  },
  "author": "Thanaen",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^29.5.3",
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "@typescript-eslint/parser": "^6.1.0",
    "esbuild": "^0.18.15",
    "eslint": "^8.45.0",
    "eslint-plugin-jest": "^27.2.3",
    "husky": "^8.0.3",
    "jest": "^29.3.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.1",
    "standard-version": "^9.5.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.1.6"
  },
  "peerDependencies": {
    "@sentry/browser": "^7.9.0",
    "zustand": ">= 4.0.0"
  }
}
